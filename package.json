{
  "name": "n8n-nodes-tumblr-auto-social",
  "version": "1.0.0",
  "description": "N8N node for Tumblr auto login and posting",
  "keywords": [
    "n8n",
    "tumblr",
    "social",
    "automation"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nguyenkhoi489/n8n-tumblr-package.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TumblrApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TumblrAutoSocial/TumblrAutoSocial.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "gulp": "^5.0.1",
    "gulp-cli": "^3.1.0",
    "n8n-workflow": "*",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "oauth": "^0.10.0",
    "tumblr.js": "^3.0.0"
  }
}
